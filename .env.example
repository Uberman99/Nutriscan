# Copy this file to .env.local and add your actual values

# Google AI (Gemini) API Key
GOOGLE_API_KEY=your_google_ai_api_key_here

# Google Cloud Platform Credentials (for Vision API)
GCP_CREDENTIALS=your_gcp_credentials_json_string_here

# Vercel Postgres Database (for production deployment)
POSTGRES_URL=your_postgres_url_here
POSTGRES_PRISMA_URL=your_postgres_prisma_url_here
POSTGRES_URL_NON_POOLING=your_postgres_url_non_pooling_here
POSTGRES_USER=your_postgres_user_here
POSTGRES_HOST=your_postgres_host_here
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DATABASE=your_postgres_database_here

# Clerk Authentication (if you want to enable authentication)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here
