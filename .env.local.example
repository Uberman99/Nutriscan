# Environment Variables for NutriScan
# Copy this file to .env.local and fill in your actual values

# Clerk Authentication (Required for user authentication)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# Vercel Postgres Database (Optional - will use mock data if not configured)
POSTGRES_URL="postgres://default:***@***.***/verceldb?sslmode=require"
POSTGRES_PRISMA_URL="postgres://default:***@***.***/verceldb?sslmode=require&pgbouncer=true&connect_timeout=15"
POSTGRES_URL_NO_SSL="postgres://default:***@***.***/verceldb"
POSTGRES_URL_NON_POOLING="postgres://default:***@***.***/verceldb?sslmode=require"
POSTGRES_USER="default"
POSTGRES_HOST="***"
POSTGRES_PASSWORD="***"
POSTGRES_DATABASE="verceldb"

# API Keys
NUTRITIONIX_APP_ID=5bd6bc8e
NUTRITIONIX_API_KEY=b5c59ecf0273fe46c26dae5d
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here
CLARIFAI_API_KEY=your_clarifai_api_key_here

# USDA API (free, no key required)
# USDA_API_KEY=your_usda_api_key_here_if_you_have_one
